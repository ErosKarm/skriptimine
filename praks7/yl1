#palun sisestada vanuse
echo -n "Palun sisestage siia oma vanus: "
read vanus
#loen vanuse
#if-i abil teen valemi, mis  ütleb, kes ta on vanuse poolest
if test $vanus -gt 0 -a $vanus -lt 11
then
echo "oled laps"
elif test $vanus -ge 11 -a $vanus -lt 18
then
echo "oled nooruk"
elif test $vanus -ge 18 -a $vanus -lt 63
then 
echo "oled täiskasvanu"
elif test $vanus -ge 63
then
echo "oled senioor"
else 
echo "vigane sisend"
fi
#skripti lõpp
#
