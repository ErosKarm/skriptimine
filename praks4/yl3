#/bin/bash
#k체sime vajalike andmeid kasutaja k채est
echo -n "Sisesta reisjate arv grupis: "
read reisjad
echo -n "Sisesta kohtade arv bussis: "
read kohad
#kalkuleerime
x=`expr $kohad - $reisjad`
#kuvame vastuse
echo "Vaba kohti on veel bussis: $x"
echo "Kui arv on - miinuses siis t채hendab, et nii palju inimesi j채i maha."
